{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["@typescript-eslint", "prettier", "vue"],
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "vue/no-unused-vars": "error"
    }
}
